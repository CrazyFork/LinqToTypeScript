{
    "extends": "tslint:recommended",
    "rules": {
        "semicolon": [
            true, "never"
        ],
        "member-ordering": [
            false
        ],
        "max-classes-per-file": [
            false
        ],
        "unified-signatures": [ false ],
        "prefer-for-of": false,
        "callable-types": [ false ]
    }
}