{
    "extends": "tslint:recommended",
    "rules": {
        "semicolon": [
            true, "never"
        ],
        "member-ordering": [
            true
        ],
        "max-classes-per-file": [
            false
        ],
        "unified-signatures": false,
        "prefer-for-of": false,
        "callable-types": false,
        "member-access": true,
        "object-literal-sort-keys": true,
        "no-unnecessary-type-assertion": true,
        "prefer-const": true,
        "no-duplicate-imports": true
    }
}